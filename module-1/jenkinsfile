pipeline {
    agent any  
    stages {
        stage('Prepare') {
                    
            steps {
                checkout scm
            }
        }

        stage('coppy web to s3') {
                    
            steps {
                sh '''
                    echo $PWD
                    BUCKETNAME = tuewebbucket1
                    aws s3 cp /module-1/web/index.html s3://$BUCKETNAME/index.html
                '''
            }
        }
        
    }
}
